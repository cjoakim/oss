{
    "name":    "m26-js",
    "version": "0.4.0",
    "description": "A Node.js library for speed and pace calculations for sports like running and cycling. Age-graded times and heart-rate training-zones are also supported.",
    "homepage": "https://github.com/cjoakim/oss/tree/main/m26-js-ts",

    "main": "dist/index.js",
    "types": "dist/index.d.ts",

    "engines": {
        "node": ">=18.7.0"
    },
    "scripts": {
        "build": "tsc",
        "prebuild": "rimraf dist/",
        "test": "jest --verbose"
    },
    "keywords": [
        "m26",
        "running",
        "cycling",
        "swimming",
        "calculations",
        "heart-rate",
        "mph",
        "pace-per-mile",
        "age-graded"
    ],
    "author": "Chris Joakim",
    "license": "ISC",
    "dependencies": {
    },
    "devDependencies": {
        "typescript": "^5.2.2",
        "@types/jest": "^29.5.4",
        "@types/node": "^18.17.1",
        "jest": "^29.7.0",
        "rimraf": "5.0.1",
        "ts-jest": "^29.1.1"
    }
}
